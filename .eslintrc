{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "react",
    "jsx-a11y",
    "import",
    "prettier"
  ],
  "globals": {
    "__DEV__": true
  },
  "rules": {
    "arrow-parens": 0,
    "global-require": 0,
    "import/prefer-default-export": 0,
    "no-console": 0,
    "no-plusplus":0,
    "no-mixed-operators": 0,
    "no-use-before-define": 0,
    "no-underscore-dangle":0,
    "no-unused-expressions":0,
    "no-return-assign":0,
    "array-callback-return":0,
    "radix": 0,
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "react/sort-comp":0,
    "react/require-default-props":0,
    "react/no-array-index-key":0,
    "semi": [2, "never"]
  }
}
